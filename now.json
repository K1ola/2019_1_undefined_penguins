{
    "version": 2,
    "name": "corsairs",
    "alias": "penguins-corsairs",
    "builds": [
        { "src": "/server/*.js", "use": "@now/node-server" },
        { "src": "/public/**", "use": "@now/static" }
    ],
    "routes": [
        { "src": "/", "dest": "/public/index.html"},
        { "src": "/(.*)", "dest": "/public/$1" }               
      ],
    "github": {
        "enabled": false
    }
    
}
